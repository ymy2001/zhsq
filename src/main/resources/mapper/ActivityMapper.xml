<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhsq.mapper.ActivityMapper">

    <resultMap type="com.zhsq.pojo.Activity" id="ActivityMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="activityName" column="activity_name" jdbcType="VARCHAR"/>
        <result property="startTime" column="start_time" jdbcType="TIMESTAMP"/>
        <result property="endTime" column="end_time" jdbcType="TIMESTAMP"/>
        <result property="activityTitle" column="activity_title" jdbcType="VARCHAR"/>
        <result property="activityDetail" column="activity_detail" jdbcType="VARCHAR"/>
        <result property="activityImage" column="activity_image" jdbcType="VARCHAR"/>
    </resultMap>

    
</mapper>

